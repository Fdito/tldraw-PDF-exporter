{
  "name": "tldraw PDF exporter",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Creates a 'export to pdf' button",
  "icons": {
    "16": "./img/logo/tldraw16.png",
    "32": "./img/logo/tldraw32.png",
    "48": "./img/logo/tldraw48.png",
    "128": "./img/logo/tldraw128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://www.tldraw.com/*"],
      "js": ["./js/pagescript.js","./js/html2canvas.min.js","./js/jspdf.umd.min.js"]
    }
  ]
}